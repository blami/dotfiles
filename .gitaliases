; ~/.gitaliases - Useful Git aliases

[alias]
	# Short aliases
	cl = clone
	pu = push
	pf = push --force
	ci = commit
	co = checkout
	cob = checkout -b
	df = diff
	dc = diff --cached
	st = status
	br = branch
	re = rebase
	sh = stash
	rs = reset
	un = reset HEAD
	fe = fetch
	pl = pull

	# File aliases
	unstage = reset HEAD
	diffc = diff --cached

	# Log aliases
	log1 = log -p1


; vim:set ft=gitconfig:
