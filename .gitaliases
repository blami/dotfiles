; ~/.gitaliases - useful Git aliases

[alias]
    # Short aliases
    cl = clone
    pu = push
    ci = commit
    co = checkout
    df = diff
    dc = diff --cached
    dh = diff HEAD^ HEAD
    st = status
    br = branch
    re = rebase
    sh = stash
    rs = reset
    un = reset HEAD

    # File aliases
    unstage = reset HEAD
    diffc = diff --cached

    # Log aliases
    log1 = log -p1


; vim:set ft=gitconfig:
